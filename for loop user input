//user input
#include<stdio.h>
int main () {
	int i, start,sum=0,stop;
	printf("Enter a value for when to start\n");
	scanf("%d",&start);
	
	printf("Enter a value for when to stop\n");
	scanf("%d",&stop);
	
	for(i=start;i<=stop;i++)
	{
printf("%d\n",i);
sum=sum+i;
	}
	printf("The sum is %d",sum);
	
	
	return 0;
}
